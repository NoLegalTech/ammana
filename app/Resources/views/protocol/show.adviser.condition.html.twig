<style>
    #element_{{ id }} {
        display: none;
    }
</style>
{% for key in variables|keys %}
    {% if key ~ "=" ~ variables[key] == tag %}
        {% if previous == 'li' %}
            <style>
                #element_{{ id }} {
                    display: list-item;
                }
            </style>
        {% else %}
            <style>
                #element_{{ id }} {
                    display: block;
                }
            </style>
        {% endif %}
    {% endif %}
{% endfor %}
